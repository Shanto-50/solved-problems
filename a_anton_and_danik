#include <stdio.h>
#include <string.h>
int main()
{
    int x;
    scanf("%d", &x);
    char str[x];
    scanf("%s", str);
    int count1 = 0, count2 = 0;
    for (int i = 0; i < strlen(str); i++)
    {
        if (str[i] == 'A')
            count1++;
        else if (str[i] == 'D')
            count2++;
    }
    if (count1 > count2)
        printf("Anton");
    else if (count1 < count2)
        printf("Danik");
    else if (count1 == count2)
        printf("Friendship");
    return 0;
}
